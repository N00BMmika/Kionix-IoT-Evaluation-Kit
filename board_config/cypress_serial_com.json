{
    "structure_version": 1.0,
    "protocol_version": 1.2,
    "configuration": {
        "name": "Cypress CY8CKIT-059",
        "doc": "Cypress CY8CKIT-059 with KX122, KMX62, or KXG07",
        "bus1": {
            "connections": [{
                "connection": "i2c",
                "part_list": [{
                    "name": "KX122",
                    "SAD": 31,
                    "int1_gpio": 0
                },
                {
                    "name": "KMX62",
                    "SAD": 15,
                    "int1_gpio": 9
                },
                {
                    "name": "KXG08",
                    "SAD": 78,
                    "int1_gpio": 0
                }]
            }]
        },
        "bus2": {
            "connections": [{
                "connection": "USB",
                "packet_per_second": 400,
                "baud_rate": 1000000,
                "start_up_delay_ms": 2000,
                "com_port_name": ["Cypress USB UART", "USB Serial Device"]
            }]
        },
        "stream_config": {
            "directory": "stream_config/board_cypress_serial_com"
        },
        "board_init": {
            "reg_write": []
        }
    }
}
